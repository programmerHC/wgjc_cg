<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wgjc.param.dao.ParamMapper">
	<select id="getParamByCode" parameterType="java.lang.String" resultType="com.wgjc.param.entity.Param">
		select * from param where code = #{code}
	</select>
</mapper>